{
  "name": "MudamosMobile",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "10.22.x",
    "yarn": "^1.2.4"
  },
  "scripts": {
    "android": "android/gradlew -p android installDevelopmentDebug && adb reverse tcp:8081 tcp:8081 && adb shell am start -n br.com.tagview.petition.development/org.mudamos.petition.MainActivity",
    "android-beta": "ENVFILE=.env.beta android/gradlew -p android installMudamosBetaDebug && adb reverse tcp:8081 tcp:8081 && adb shell am start -n br.com.tagview.petition.mudamos.beta/org.mudamos.petition.MainActivity",
    "build-android-beta": "ENVFILE=.env.beta android/gradlew -p android assembleMudamosBetaRelease",
    "build-android-production": "ENVFILE=.env.production android/gradlew -p android assembleProductionRelease",
    "build-ios-beta": " ipa build -w ios/MudamosMobile.xcworkspace -s 'MudamosMobileBeta' -d ios/ipa --ipa MudamosTagviewBeta.ipa",
    "distribute-ios-beta": "ipa distribute:s3 -b mudamos-mobile-beta -f ios/ipa/MudamosTagviewBeta.ipa",
    "fix-ios-glob-issue": "cd node_modules/react-native && ./scripts/ios-install-third-party.sh && cd - && cd node_modules/react-native/third-party/glog-0.3.4/ && ./../../scripts/ios-configure-glog.sh && cd -",
    "fix-libfishhook-ios-issue": "curl https://gist.githubusercontent.com/guimello/8abe73c78f829796feecb477c18f8992/raw/8a0c1cefeb7992bd268d90a851af5ba39f7dfa52/fix.txt && echo '\n\nFa√ßa isso acima.'",
    "generate-ios-action-sign-html": "cd action-sign-generator && yarn run build && cd -",
    "install-action-sign-deps": "cd action-sign-generator && yarn install && yarn run install-mudamos-libcrypto-deps && cd -",
    "install-android-beta": "ENVFILE=.env.beta android/gradlew -p android installMudamosBetaRelease",
    "install-android-production": "ENVFILE=.env.production android/gradlew -p android installProductionRelease",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "ios-4s-beta": "react-native run-ios --simulator='iPhone 4s' --scheme MudamosMobileBeta",
    "ios-se-beta": "react-native run-ios --simulator='iPhone SE iOS 10' --scheme MudamosMobileBeta",
    "ios-4s-dev": "react-native run-ios --simulator='iPhone 4s' --scheme MudamosMobile",
    "ios-beta": "react-native run-ios --scheme MudamosMobileBeta",
    "lint": "eslint src/ main.*.js index.js",
    "list-ios-simulators": "xcrun simctl list devices",
    "postinstall": "./node_modules/.bin/rn-nodeify -y --install vm,buffer,stream,events,crypto,assert,process,react-native-randombytes,react-native-crypto --hack; jetify; patch-package",
    "reset-adb": "adb kill-server; sleep 2; adb start-server && adb reverse tcp:8081 tcp:8081 && adb devices",
    "start": "react-native start",
    "test": "mocha --compilers js:babel-core/register --require spec/support.js 'spec/**/*.spec.js'",
    "fix-xcode-12-valid-archs": "find node_modules/ -name \"project.pbxproj\" -exec sed -i '' '/VALID_ARCHS/d' {} +"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.1.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-firebase/analytics": "^10.1.1",
    "@react-native-firebase/app": "^10.1.0",
    "assert": "^1.4.1",
    "buffer": "^5.1.0",
    "deprecated-react-native-listview": "^0.0.6",
    "diacritics": "^1.3.0",
    "es6-error": "^4.0.0",
    "events": "^2.0.0",
    "farfetch": "0.7.0",
    "humps": "^2.0.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.20",
    "marked": "^0.3.6",
    "moment": "^2.16.0",
    "mudamos-libcrypto": "0.6.4",
    "numeral": "^2.0.4",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "process": "^0.11.9",
    "prop-types": "^15.7.2",
    "ramda": "0.26.1",
    "react": "16.13.1",
    "react-addons-shallow-compare": "15.6.2",
    "react-native": "0.63.4",
    "react-native-animatable": "1.3.3",
    "react-native-animate-number": "^0.1.2",
    "react-native-collapsible": "1.5.3",
    "react-native-config": "https://github.com/itsriodejaneiro/react-native-config.git#master",
    "react-native-crypto": "2.1.2",
    "react-native-device-info": "0.21.2",
    "react-native-drawer": "^2.5.1",
    "react-native-extended-stylesheet": "^0.9.0",
    "react-native-fbsdk": "2.0.0",
    "react-native-fence-html": "https://github.com/itsriodejaneiro/react-native-fence-html.git#9eb096c",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-image-picker": "^0.26.7",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "0.27.1",
    "react-native-material-kit": "https://github.com/itsriodejaneiro/react-native-material-kit.git#mudamos",
    "react-native-onesignal": "^3.9.3",
    "react-native-open-settings": "https://github.com/itsriodejaneiro/react-native-open-settings.git#952440d",
    "react-native-permissions": "^1.1.1",
    "react-native-randombytes": "^3.6.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-router-flux": "3.45.0",
    "react-native-share": "1.2.1",
    "react-native-simple-toast": "https://github.com/itsriodejaneiro/react-native-simple-toast.git#52a1268",
    "react-native-spinkit": "^1.5.1",
    "react-native-tab-view": "2.15.2",
    "react-native-user-defaults": "https://github.com/itsriodejaneiro/react-native-user-defaults.git#ce490d2",
    "react-native-vector-icons": "4.6.0",
    "react-native-webview": "^11.0.0",
    "react-redux": "7.2.2",
    "readable-stream": "^2.3.5",
    "recompose": "0.30.0",
    "redux": "^3.6.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "rn-viewpager": "https://github.com/itsriodejaneiro/React-Native-ViewPager.git#801f264",
    "sjcl": "^1.0.7",
    "stream-browserify": "^2.0.1",
    "string-mask": "^0.3.0",
    "vm-browserify": "0.0.4"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^25.1.0",
    "chai": "^4.1.2",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^25.1.0",
    "jest-react-native": "18.0.0",
    "jetifier": "^1.6.6",
    "metro-react-native-babel-preset": "^0.59.0",
    "mocha": "^5.0.4",
    "react-test-renderer": "16.13.1",
    "rn-nodeify": "^10.0.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "classes": true,
        "jsx": true,
        "modules": true
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "fetch": false,
      "require": false
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "@react-native-community"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-console": "off",
      "quotes": [
        "error",
        "double"
      ],
      "react/sort-prop-types": [
        "warn",
        {
          "callbacksLast": true
        }
      ],
      "strict": [
        "error",
        "global"
      ]
    }
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  }
}
