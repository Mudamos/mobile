{
  "name": "MudamosMobile",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "8.9.x",
    "npm": "3.10.x"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "android/gradlew -p android installDevelopmentDebug && adb reverse tcp:8081 tcp:8081 && adb shell am start -n br.com.tagview.petition.development/org.mudamos.petition.MainActivity",
    "android-beta": "ENVFILE=.env.beta android/gradlew -p android installMudamosBetaDebug && adb reverse tcp:8081 tcp:8081 && adb shell am start -n br.com.tagview.petition.mudamos.beta/org.mudamos.petition.MainActivity",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "ios-beta": "node node_modules/react-native/local-cli/cli.js run-ios --scheme MudamosMobileBeta",
    "ios-4s-beta": "node node_modules/react-native/local-cli/cli.js run-ios --simulator='iPhone 4s' --scheme MudamosMobileBeta",
    "ios-se-beta": "node node_modules/react-native/local-cli/cli.js run-ios --simulator='iPhone SE iOS 10' --scheme MudamosMobileBeta",
    "ios-4s-dev": "node node_modules/react-native/local-cli/cli.js run-ios --simulator='iPhone 4s' --scheme MudamosMobile",
    "reset-adb": "adb kill-server; sleep 2; adb start-server && adb reverse tcp:8081 tcp:8081 && adb devices",
    "list-ios-simulators": "xcrun simctl list devices",
    "lint": "eslint src/ main.*.js index.js",
    "test": "mocha --compilers js:babel-core/register --require spec/support.js 'spec/**/*.spec.js'",
    "build-ios-beta": " ipa build -w ios/MudamosMobile.xcworkspace -s 'MudamosMobileBeta' -d ios/ipa --ipa MudamosTagviewBeta.ipa",
    "build-android-beta": "yarn run clean-build-files && yarn run android-bundle && ENVFILE=.env.beta android/gradlew -p android assembleMudamosBetaRelease && yarn run clean-build-files",
    "build-android-production": "yarn run clean-build-files && yarn run android-bundle && ENVFILE=.env.production android/gradlew -p android assembleProductionRelease && yarn run clean-build-files",
    "clean-build-files": "./bin/clean-android-build-files",
    "distribute-ios-beta": "ipa distribute:s3 -b mudamos-mobile-beta -f ios/ipa/MudamosTagviewBeta.ipa",
    "install-android-beta": "yarn run clean-build-files && yarn run android-bundle && ENVFILE=.env.beta android/gradlew -p android installMudamosBetaRelease && yarn run clean-build-files",
    "postinstall": "./node_modules/.bin/rn-nodeify -y --install vm,buffer,stream,events,crypto,assert,process,react-native-randombytes,react-native-crypto --hack",
    "install-action-sign-deps": "cd action-sign-generator && yarn install && yarn run install-mudamos-libcrypto-deps && cd -",
    "generate-ios-action-sign-html": "cd action-sign-generator && yarn run build && cd -"
  },
  "dependencies": {
    "@shoutem/animation": "^0.12.2",
    "assert": "^1.4.1",
    "buffer": "^5.1.0",
    "diacritics": "^1.3.0",
    "es6-error": "^4.0.0",
    "events": "^2.0.0",
    "farfetch": "0.7.0",
    "humps": "^2.0.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "moment": "^2.16.0",
    "mudamos-libcrypto": "0.6.4",
    "numeral": "^2.0.4",
    "process": "^0.11.9",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "16.3.1",
    "react-addons-shallow-compare": "15.6.2",
    "react-native": "0.55.4",
    "react-native-animatable": "^1.1.0",
    "react-native-animate-number": "^0.1.2",
    "react-native-collapsible": "^0.13.0",
    "react-native-config": "^0.11.5",
    "react-native-crypto": "2.1.2",
    "react-native-device-info": "0.21.2",
    "react-native-drawer": "^2.3.0",
    "react-native-extended-stylesheet": "^0.9.0",
    "react-native-fbsdk": "0.8.0",
    "react-native-fence-html": "https://github.com/itsriodejaneiro/react-native-fence-html.git#d534ffc",
    "react-native-firebase-crash-report": "https://github.com/itsriodejaneiro/react-native-firebase-crash-report.git#mudamos",
    "react-native-firestack": "https://github.com/itsriodejaneiro/react-native-firestack.git#5ee20d7",
    "react-native-image-picker": "^0.26.7",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-maps": "0.18.3",
    "react-native-material-kit": "^0.5.1",
    "react-native-onesignal": "https://github.com/itsriodejaneiro/react-native-onesignal.git#mudamos",
    "react-native-open-settings": "https://github.com/itsriodejaneiro/react-native-open-settings.git#952440d",
    "react-native-permissions": "^1.1.1",
    "react-native-randombytes": "^3.0.0",
    "react-native-router-flux": "3.43.0",
    "react-native-share": "1.0.26",
    "react-native-simple-toast": "https://github.com/itsriodejaneiro/react-native-simple-toast.git#ef1c167",
    "react-native-spinkit": "^1.1.1",
    "react-native-tab-view": "^1.0.0",
    "react-native-user-defaults": "https://github.com/itsriodejaneiro/react-native-user-defaults.git#ce490d2",
    "react-native-vector-icons": "4.6.0",
    "react-redux": "5.0.7",
    "readable-stream": "^2.3.5",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "rn-viewpager": "^1.1.4",
    "sjcl": "^1.0.7",
    "stream-browserify": "^2.0.1",
    "string-mask": "^0.3.0",
    "vm-browserify": "0.0.4"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "22.4.1",
    "babel-preset-react-native": "^4.0.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "22.4.2",
    "jest-react-native": "18.0.0",
    "mocha": "^5.0.4",
    "nodegit": "^0.24.1",
    "react-test-renderer": "16.2.0",
    "rn-nodeify": "^10.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "classes": true,
        "jsx": true,
        "modules": true
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "fetch": false,
      "require": false
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-console": "off",
      "quotes": [
        "error",
        "double"
      ],
      "react/sort-prop-types": [
        "warn",
        {
          "callbacksLast": true
        }
      ],
      "strict": [
        "error",
        "global"
      ]
    }
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  }
}
